{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "",
  "repository": "https://github.com/themeblvd/mern-boilerplate.git",
  "scripts": {
    "start": "concurrently 'NODE_ENV=development webpack --config webpack.config.client.js --watch' 'npm run server'",
    "build": "npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config webpack.config.client.js",
    "build:server": "NODE_ENV=production webpack --config webpack.config.server.js",
    "client": "NODE_ENV=development webpack --config webpack.config.client.js",
    "server": "NODE_ENV=development nodemon index.js",
    "server:prod": "NODE_ENV=production nodemon index.js",
    "js-lint": "eslint \"{*.js,server/**/*.js,client/*.js,client/components/*.js,client/store/*.js}\"",
    "js-lint:fix": "eslint \"{*.js,server/**/*.js,client/*.js,client/components/*.js,client/store/*.js}\" --fix",
    "format": "prettier --write --single-quote --print-width=100 --tab-width=2 \"{*.js,server/**/*.js,client/*.js,client/components/*.js,client/store/*.js}\"",
    "report": "concurrently 'npm run report:client' 'npm run report:server'",
    "report:client": "NODE_ENV=development webpack --config webpack.config.client.js --json | webpack-bundle-size-analyzer >> ./report-client.txt",
    "report:server": "NODE_ENV=development webpack --config webpack.config.server.js --json | webpack-bundle-size-analyzer >> ./report-server.txt"
  },
  "author": "Jason Bobich",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "morgan": "^1.9.1",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.13.5",
    "sass-loader": "^7.1.0",
    "webpack": "^4.20.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.1.1",
    "webpack2-externals-plugin": "^1.0.0"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
